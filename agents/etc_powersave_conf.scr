/**
 * File:        etc_cups_printers.scr
 * Summary:     Agent for reading /etc/cups/printers.conf file
 * Author:      Jiri Srain <jsrain@suse.cz>
 * Access:      read
 *
 * $Id$
 *
 */
.etc.powersave

`ag_ini(
    `IniAgent( "/etc/powersave.conf",
	$[
	    "options"	: [ "global_values" ],
	    "comments"	: [ "^[ \t]*#.*$", "^[ \t]*$" ],
	    "params"    : [
		$[ "match" : [ "^[ \t]*([^ \t]+)[ \t]*=[ \t]*(.*)$", "%s=%s" ]],
	    ],
	]
))


